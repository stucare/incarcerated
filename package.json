{
  "name": "incarcerated",
  "version": "0.1.0",
  "description": "incarcerated website",
  "main": "./server/server.js",
  "scripts": {
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "nodemon --exec \"npm test\"",
    "less:compile": "lessc ./wwwroot/css/inc.less ./wwwroot/css/inc.css",
    "less:compile:watch": "nodemon --exec \"npm run less:compile\" -e less",
    "start": "node ./server/server.js",
    "start:watch": "nodemon ./server/server.js -e js,json,hbs"
  },
  "engines": {
    "node": "10.5.0"
  },
  "author": "StuCare <mail@stuartcare.com>",
  "license": "ISC",
  "devDependencies": {
    "expect": "^23.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "rewire": "^4.0.1",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "jsonwebtoken": "^8.3.0",
    "less": "^3.7.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^3.1.0",
    "mongoose": "^5.2.0",
    "socket.io": "^2.1.1",
    "validator": "^10.4.0",
    "yargs": "^12.0.1"
  }
